    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-subset": "^1.6.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "http-serve": "^1.0.1",
    "husky": "0.14.3",
    "lint-staged": "4.3.0",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "^0.4.8",
    "nock": "^9.1.6",
    "prettier": "^1.16.4",
    "sinon": "1.17.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "3.5.2"
  },
  "scripts": {
    "start": "NODE_ENV=production node_modules/ts-node/dist/bin.js app.ts",
    "build": "tsc",
    "build_doc": "jsdoc app.js -d docs",
    "prettify": "node_modules/prettier/bin-prettier.js  \"**/*.{js,json}\" --write",
    "lint": "./node_modules/tslint/bin/tslint {admin,proxy,test,storage,utils}/**/*.ts",
    "test": "JUNIT_REPORT_PATH=junit.xml nyc mocha -r ts-node/register test/*/* --reporter mocha-jenkins-reporter --exit",
    "pretest": "lint-staged && npm run lint",
    "precommit": "lint-staged",
    "update_template_models": "NODE_ENV=production node_modules/ts-node/dist/bin.js utils/updateFSTemplateModels.ts",
    "refactor_storage": "NODE_ENV=production node_modules/ts-node/dist/bin.js refactorStorage.ts"
  },
  "nyc": {
    "check-coverage": false,
    "extension": [
      ".js",
      ".ts"
    ],
    "exclude": [
      "xmlns",
      "dist",
      "test"
    ],
    "reporter": [
      "cobertura",
      "html"
    ]
  },
  "lint-staged": {
    "*.{js,json}": [
      "node_modules/prettier/bin-prettier.js --write",
      "git add"
    ],
    "*.ts": [
      "./node_modules/tslint/bin/tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://bbpcode.epfl.ch/neurorobotics/nrpBackendProxy",
    "issuesurl": "https://hbpneurorobotics.atlassian.net/"
  },
  "keywords": [
    "Proxy"
  ],
  "engines": {
    "node": ">=7"
  },
