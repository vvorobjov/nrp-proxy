{
  "name": "nrp_backend_proxy",
  "version": "4.0.0",
  "description": "Keep an updated list of available and joinable servers for each experiment",
  "main": "src/app",
  "dependencies": {
    "@types/express": "4.17.0",
    "@types/express-serve-static-core": "4.16.7",
    "@types/lodash": "^4.14.117",
