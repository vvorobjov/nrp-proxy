{
  "nrpVersion": "3.2.1",
  "refreshInterval": 5000,
  "bucketUrlApi": "https://data-proxy-int.ebrains.eu/api/v1/buckets",
  "searchTagUrl": "https://wiki-int.ebrains.eu/rest/wikis/xwiki/tags/",
  "auth": {
    "renewInternal": 600000,
    "clientId": "some-client-id",
    "clientSecret": "secret",
    "url": "https://authentication.eu",
    "deactivate": true
  },
  "port": 8000,
  "modelsPath": "$HBP/Models",
  "templatesPath": "$HBP/Experiments",
  "servers": {
    "localhost": {
      "gzweb": {
        "assets": "http://localhost:8080/assets",
        "nrp-services": "http://localhost:8080",
        "videoStreaming": "http://localhost:8080/webstream/",
        "websocket": "ws://localhost:8080/gzbridge"
      },
      "rosbridge": {
        "websocket": "ws://localhost:8080/rosbridge"
      },
      "serverJobLocation": "local"
    }
  },
  "storage": "FS",
  "authentication": "FS",
  "restart-backend-cmd": "inexisting-script"
}
